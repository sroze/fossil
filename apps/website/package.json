  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "npx remix setup node",
    "start": "npx remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.9",
    "@heroicons/react": "^2.0.14",
    "@remix-run/react": "^1.6.0",
    "@remix-run/serve": "^1.6.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "md5": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.4.0",
    "remix-auth-auth0": "^1.5.1",
    "remix-validated-form": "^4.6.8",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.0",
    "@types/md5": "^2.3.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.6.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
