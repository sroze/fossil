{
  "name": "website",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "npx remix setup node",
    "start": "npx remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.9",
    "@heroicons/react": "^2.0.14",
    "@opentelemetry/api": "^1.4.0",
    "@opentelemetry/core": "^1.9.1",
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "event-store": "workspace:*",
    "luxon": "^3.2.1",
    "md5": "^2.3.0",
    "node-jose": "^2.1.1",
    "pg": "^8.9.0",
    "pg-format": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.7.0",
    "remix-auth": "^3.4.0",
    "remix-auth-auth0": "^1.5.1",
    "remix-utils": "^6.0.0",
    "remix-validated-form": "^4.6.8",
    "sql-template-tag": "^4.1.0",
    "store-security": "workspace:*",
    "subscription": "workspace:*",
    "store-locator": "workspace:*",
    "event-serialization": "workspace:*",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@types/luxon": "^3.2.0",
    "@types/md5": "^2.3.2",
    "@types/pg": "^8.6.6",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/uuid": "^9.0.0",
    "concurrently": "^7.6.0",
    "tailwindcss": "^3.2.4",
    "tsconfig": "workspace:*",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
