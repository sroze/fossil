{
  "name": "read-models",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles",
    "migrate": "pg-migrations apply --directory ./migrations"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@databases/pg-migrations": "^5.0.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "event-store": "workspace:*",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.9.0",
    "sql-template-tag": "^4.1.0",
    "subscription": "workspace:*"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "workspace:*",
    "jest": "^29.3.1",
    "jest-presets": "workspace:*",
    "supertest": "^6.2.4",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
